#include<bits/stdc++.h>
using namespace std;

class Add{
public:
  void add(int a,int b){
    cout<<"Addition : "<<(a+b)<<endl;
  }
  void add(int a,int b,int c){
    cout<<"Subtraction : "<<(a+b+c);
  }
};

int main(){
  int a,b,c;
  cin>>a>>b>>c;
  Add object;
  object.add(a,b);
  object.add(a,b,c);
  return 0;
}
